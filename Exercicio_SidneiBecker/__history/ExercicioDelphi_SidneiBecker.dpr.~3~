program ExercicioDelphi_SidneiBecker;

uses
  Vcl.Forms,
  unitPrincipal in 'unitPrincipal.pas' {frmPrincipal},
  UnitVotacao_SidneiBecker in 'UnitVotacao_SidneiBecker.pas' {frmVotacao};

{$R *.res}

